{
  "name": "WF",
  "version": "0.1.0",
  "description": "workflow",
  "main": "main/index.js",
  "homepage": ".",
  "scripts": {
    "start": "BABEL_ENV=development webpack-dev-server -w --env=development & npm run electron",
    "build": "BABEL_ENV=production webpack --progress --env=production",
    "electron": "ELECTRON_ENV=dev electron .",
    "dist": "electron-builder",
    "package": "ELECTRON_ENV=pro electron-packager . --overwrite  --download.mirror=https://npm.taobao.org/mirrors/electron/ --icon=assets/wf.icns",
    "format": "prettier --write \"./src/**/*.js\" \"./src/**/*.jsx\" "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn format",
      "git add"
    ]
  },
  "author": {
    "name": "baolq"
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "antd": "^3.19.7",
    "babel-polyfill": "^6.26.0",
    "bundle-loader": "^0.5.6",
    "codemirror": "^5.48.0",
    "fs-extra": "^8.1.0",
    "lowdb": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "sudo-prompt": "^8.2.5"
  }
}
