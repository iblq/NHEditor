    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-react-css-modules": "^3.4.2",
