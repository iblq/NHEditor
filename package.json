{
  "name": "WF",
  "version": "0.1.0",
  "description": "workflow",
  "main": "main/index.js",
  "homepage": ".",
  "scripts": {
    "start": "BABEL_ENV=development webpack-dev-server -w --env=development & npm run electron",
    "build": "BABEL_ENV=production webpack --progress --env=production",
    "electron": "ELECTRON_ENV=dev electron .",
    "package": "ELECTRON_ENV=pro electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --download.mirror=https://npm.taobao.org/mirrors/electron/ --icon=assets/wf.icns",
    "format": "prettier --write \"./src/**/*.js\" \"./src/**/*.jsx\" ",
    "package:all": "ELECTRON_ENV=pro electron-packager . --overwrite"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn format",
      "git add"
    ]
  },
  "author": {
    "name": "baolq"
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
